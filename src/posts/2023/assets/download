getTop().templateRequestRetry(function(sid) {
var form = document.getElementById('sessionTimeoutForm');
var urlSid = (location.href.match(/[\?&]sid=(.*?)(&|#|$)/) || [])[1] || '';
var url = location.href.replace(urlSid, sid);
console.log('this is form', form);
if (form) {
var inputs = form.getElementsByTagName('input');
for (var i = 0; i < inputs.length; i++) {
var input = inputs[i]
if (input.name === 'sid') {
input.value = sid
}
}
form.action = url
getTop().tdw({module: 'login', action: 'formSubmit'});
form.submit();
} else if (sid === urlSid) {
getTop().tdw({module: 'login', action: 'locationReload'});
location.reload();
} else {
getTop().tdw({module: 'login', action: 'locationReplace'});
location.replace(url);
}
})
